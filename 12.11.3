#Программа находит во входном потоке простые числа. Поток заканчивается'!'. 
a=True
k=0
n=[]
while n!='!':
    n=input()
    if n.isdigit()==a:
        n=int(n)
        if n<=2:
            if n!=1 and n!=0 and n==2:
                print(n,':prime')
        else:
            for i in range(2, n):
                if n%i==0:
                    k=1
            if k==1:
                print(n,':composite')
            else:
                print(n,':prime')
    elif n!='!':
        print(n,':not a number')
input()
